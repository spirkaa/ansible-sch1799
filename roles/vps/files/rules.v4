*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
-A FORWARD -i wg0 -j ACCEPT
COMMIT

*nat
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
-A PREROUTING -d 213.183.53.156/32 -p tcp -m multiport --dports 80,443 -j DNAT --to-destination 192.168.254.2
-A POSTROUTING -o eth0 -j MASQUERADE
COMMIT
